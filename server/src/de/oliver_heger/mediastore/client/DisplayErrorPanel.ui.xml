<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:style>
    .errpnl {
    	font-weight: bold;
    }
  </ui:style>
  <g:DisclosurePanel ui:field="panel" addStyleNames="{style.errpnl}"
    visible="false">
    <g:header>Error when retrieving data!</g:header>
    <g:HTML ui:field="labContent"/>
  </g:DisclosurePanel>
</ui:UiBinder>
