<!--
  Global bean definitions for the LineDJ archive startup application.
-->
<j:jelly xmlns:j="jelly:core" xmlns:di="diBuilder">

  <!-- The specialized window manager -->
  <di:bean name="jguiraffe.windowManager"
           beanClassName="de.oliver_heger.linedj.platform.app.JavaFxSharedWindowManager">
    <di:constructor>
      <di:param refName="jguiraffe.styleSheetProvider"/>
    </di:constructor>
  </di:bean>
</j:jelly>